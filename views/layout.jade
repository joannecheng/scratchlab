!!!
html
  head
    title= title
    script(type='text/javascript')
      window.handlerTypes = [];
    script(src='/socket.io/socket.io.js')
    != js('jquery-1.8.2.min.js')
    != js('client.js')
    != js('timeline.js')
    != js('scatter.js')
    != js('basictext.js')
    script(src='http://cdnjs.cloudflare.com/ajax/libs/underscore.js/1.4.1/underscore-min.js')
    script(src='http://d3js.org/d3.v3.min.js')
    != css('style.css')


  body
    div(class='flash')
    header
      - console.log(session)
      - if (session.user != undefined)
        img(src=session.avatar)
        span 
          #{session.user}
          a(href="/new") Create a new Channel
      - else
        a(href="/login") Login with Github
    div(class='content')
      block content
  
